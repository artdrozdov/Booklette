@using Booklette.Models
@model ConsoleModel

@{Html.RenderPartial("_menu");}

<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12"><br/>
        <form method="post" action="@Url.Action("UploadBook","Console")" enctype="multipart/form-data">
            <div class="input-group">
                <input type="file" class="form-control" name="Book"/>
                <span class="input-group-btn">
                    <button type="submit" class="btn btn-default">Upload</button>
                </span>
            </div>
        </form>
    </div>
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
        <h1>
        @Html.Raw(Model.Message)
    </h1>
    </div>
</div>